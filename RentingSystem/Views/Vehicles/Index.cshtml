@model List<RentingSystem.Controllers.Vehicle>

<h1>Available Vehicles</h1>

<table>
    <thead>
        <tr>
            <th>Vehicle ID</th>
            <th>Vehicle Name</th>
            <th>Description</th>
            <th>License Plate</th>
            <th>Fuel Type</th>
            <th>Fuel Tank Capacity</th>
            <th>Fuel Level</th>
            <th>License To Operate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var vehicle in Model)
        {
            <tr>
                <td>@vehicle.VehicleID</td>
                <td>@vehicle.VehicleName</td>
                <td>@vehicle.Description</td>
                <td>@vehicle.LicensePlate</td>
                <td>@vehicle.FuelType</td>
                <td>@vehicle.FuelTankCapacity</td>
                <td>@vehicle.FuelLevel</td>
                <td>@vehicle.LicenseToOperate</td>
            </tr>
        }
    </tbody>
</table>